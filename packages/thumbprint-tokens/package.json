{
    "name": "@thumbtack/thumbprint-tokens",
    "description": "Design variables that power Thumbtackâ€™s UI.",
    "version": "8.3.3",
    "main": "dist/javascript-cjs/index.js",
    "module": "dist/javascript-es/index.js",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build:main": "./src/index.js",
        "build:esm": "tsc --project ./tsconfig.json --module es2015 --target es2015 --outDir dist/javascript-es",
        "build:cjs": "tsc --project ./tsconfig.json --module commonjs --target es5 --outDir dist/javascript-cjs",
        "build": "yarn build:main && yarn build:esm && yarn build:cjs",
        "clear": "rm -rf dist",
        "start": "yarn clear && yarn build",
        "prepublishOnly": "../../scripts/should-build-package.js -- yarn start"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "author": "Daniel O'Connor <daniel@danoc.me>",
    "license": "Apache-2.0",
    "devDependencies": {
        "fs-extra": "^8.1.0",
        "glob": "^7.1.2",
        "handlebars": "^4.2.0",
        "lodash": "^4.17.4"
    },
    "bugs": {
        "url": "https://github.com/thumbtack/thumbprint/issues"
    },
    "homepage": "https://github.com/thumbtack/thumbprint/blob/master/packages/thumbprint-tokens/",
    "dependencies": {
        "jszip": "^3.2.2",
        "number-to-words": "^1.2.4"
    }
}
